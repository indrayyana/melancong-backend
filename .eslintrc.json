{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "prefer-destructuring": "off",
        "arrow-body-style": "off",
        "no-unused-vars": "warn",
        "import/prefer-default-export": "off",
        "import/extensions": "off"
    }
}
